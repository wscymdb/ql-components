{
    "name": "ql-frontend",
    "version": "0.0.0",
    "description": "前端资产库，收录一些公共的组件、sdk等。基于Monorepo管理",
    "main": "index.js",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "turbo run dev",
        "dev:ui": "pnpm --filter @ql-frontend/ui dev",
        "dev:docs": "pnpm --filter @ql-frontend/docs dev",
        "dev:sdk": "pnpm --filter @ql-frontend/upload-sdk dev",
        "dev:window": "concurrently -n \"UI,SDK,DOCS\" -c \"cyan,yellow,magenta\" \"pnpm dev:ui\" \"pnpm dev:sdk\" \"sleep 5 && pnpm dev:docs\"",
        "build:docs": "pnpm --filter @ql-frontend/docs build",
        "preview:docs": "pnpm --filter @ql-frontend/docs preview",
        "build:sdk": "pnpm --filter @ql-frontend/upload-sdk build",
        "lint:es": "eslint",
        "lint:fix": "eslint --fix",
        "lint:style": "stylelint \"{packages,apps}/**/*.css\" ",
        "lint:style:fix": "stylelint \"{packages,apps}/**/*.css\"  --fix",
        "lint": "pnpm lint:es && pnpm lint:style",
        "spellcheck": "cspell --gitignore --dot --cache  --show-suggestions  --config cspell.config.js",
        "prepare": "husky",
        "commit": "git-cz"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    },
    "keywords": [],
    "author": "puta",
    "license": "ISC",
    "packageManager": "pnpm@10.13.1",
    "devDependencies": {
        "@changesets/cli": "^2.29.8",
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@eslint/js": "^9.39.1",
        "@types/react": "^18.3.27",
        "commitizen": "^4.3.1",
        "concurrently": "^9.2.1",
        "cross-env": "^10.1.0",
        "cspell": "^9.3.2",
        "cz-git": "^1.12.0",
        "eslint": "^9.39.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "globals": "^16.5.0",
        "husky": "^9.1.7",
        "jiti": "^2.6.1",
        "lint-staged": "^16.2.7",
        "prettier": "^3.7.4",
        "stylelint": "^16.26.1",
        "stylelint-config-standard": "^39.0.1",
        "turbo": "^2.6.3",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.0"
    },
    "dependencies": {
        "@ant-design/icons": "^6.1.0",
        "antd": "^6.1.0",
        "react": "^18.3.1"
    }
}
